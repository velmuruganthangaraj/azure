<div class="modal no-padding" id="application-disable-confirmation" tabindex="-1" role="dialog">
    <div class="col-lg-12 modal-dialog no-padding" role="document">
        <div class="col-lg-12 modal-content no-padding" autocomplete="off">
            <div class="col-md-12 modal-header">
                <div class="col-xs-8 no-padding">
                    <i class="su su-delete head-icon"></i>
                    <span class="modal-title">[[[Delete Application]]]</span>
                </div>
                <div class="col-xs-4 no-padding">
                    <a href="javascript:void(0);" onclick="onDisableApplicationDialogClose()" class="close"><i class="su su-close"></i></a>
                </div>
            </div>
            <div class="modal-body col-xs-12">
                <div class="col-xs-12 no-left-padding no-bottom-padding no-right-padding">
                    <div class="delete-msg">[[[Are you sure you want to delete the application]]] &mdash; <span id="disableAppName" class="highlight-name"></span> ?</div>
                </div>
            </div>
            <div class="col-xs-12 modal-footer  no-right-padding no-left-padding">
                <div class="no-padding">
                    <input type="button" id="disable-Application" class="critical-action-button" value="[[[Yes]]]" />
                    <input type="button" onclick="onDisableApplicationDialogClose()" class="secondary-button popup-close" value="[[[No]]]" />
                </div>
            </div>
        </div>
    </div>
</div>

<div class="modal no-padding" id="application-enable-confirmation" tabindex="-1" role="dialog">
    <div class="col-lg-12 modal-dialog no-padding" role="document">
        <div class="col-lg-12 modal-content no-padding" autocomplete="off">
            <div class="col-md-12 modal-header">
                <div class="col-xs-8 no-padding">
                    <i class="su su-delete head-icon"></i>
                    <span class="modal-title">[[[Enable Application]]]</span>
                </div>
                <div class="col-xs-4 no-padding">
                    <a href="javascript:void(0);" onclick="onEnableApplicationDialogClose()" class="close"><i class="su su-close"></i></a>
                </div>
            </div>
            <div class="modal-body col-xs-12">
                <div class="col-xs-12 no-left-padding no-bottom-padding no-right-padding">
                    <div class="delete-msg">[[[Are you sure you want to enable the application]]] &mdash; <span id="enableAppName" class="highlight-name"></span> ?</div>
                </div>
            </div>
            <div class="col-xs-12 modal-footer  no-right-padding no-left-padding">
                <div class="no-padding">
                    <input type="button" id="enable-Application" class="critical-action-button" value="[[[Yes]]]" />
                    <input type="button" onclick="onEnableApplicationDialogClose()" class="secondary-button popup-close" value="[[[No]]]" />
                </div>
            </div>
        </div>
    </div>
</div>